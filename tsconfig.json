{
  "name": "ppm-tool",
  "version": "0.6.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prisma:validate": "prisma validate --schema=./prisma/schema.prisma",
    "prisma:gen": "prisma generate --schema=./prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --name init --schema=./prisma/schema.prisma",
    "migrate:deploy": "prisma migrate deploy --schema=./prisma/schema.prisma",
    "seed": "node scripts/seed.cjs",
    "vercel-build": "npm run prisma:validate && npm run prisma:gen && npm run migrate:deploy"
  },
  "dependencies": {
    "@dnd-kit/core": "6.1.0",
    "@dnd-kit/modifiers": "6.0.1",
    "@dnd-kit/sortable": "8.0.0",
    "@prisma/client": "5.18.0",
    "@tanstack/react-query": "^5.0.0",
    "next": "14.2.5",
    "next-auth": "5.0.0-beta.20",
    "prisma": "5.18.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "recharts": "2.12.7",
    "swr": "2.2.5",
    "uuid": "^9.0.0",
    "zod": "^3.0.0"
  },
  "devDependencies": {
    "eslint": "^8.0.0",
    "eslint-config-next": "14.2.5",
    "typescript": "5.5.4"
  }
}
